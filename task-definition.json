{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "inferenceAccelerators": [],
    "containerDefinitions": [
        {
            "name": "ecs-devops-project",
            "image": "ecs-devops-project-repository:00000",
            "resourceRequirements": null,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": "8080",
                    "protocol": "tcp"
                }
            ],
            "secrets": [
                {
                    "name": "AWS_ACCESS_KEY",
                    "valueFrom": "arn:aws:ssm:us-east-1:993560847451:parameter/AccesskeyID"
                },
                {
                    "name": "AWS_SECRET_ACCESS_KEY",
                    "valueFrom": "arn:aws:ssm:us-east-1:993560847451:parameter/Secretaccesskey"
                }
            ],
            "environmentFiles": [],
            "secrets": null,
            "mountPoints": null,
            "volumesFrom": null,
            "hostname": null,
            "user": null,
            "workingDirectory": null,
            "extraHosts": null,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/ecs-devops-project-task-definition",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "ulimits": null,
            "dockerLabels": null,
            "dependsOn": null,
            "repositoryCredentials": {
                "credentialsParameter": ""
            }
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "4096",
    "cpu": "2048",
    "executionRoleArn": "arn:aws:iam::993560847451:role/ecsTaskExecutionRole",
    "family": "ecs-devops-project-task-definition",
    "taskRoleArn": "arn:aws:iam::993560847451:role/ecsTaskExecutionRole",
    "runtimePlatform": {
        "operatingSystemFamily": "LINUX"
    },
    "tags": []
}